def childbirth(Patientsage:int,childbirthno:int,childage:int,Patients_name:str)->int:
    Patients_name= #input
    childage = #input
    childbirthno= #input
    idealbreak= #input
    average = childage*Patientsage/childno
    birthratereduction= idealbreak-average
    print(Patients_name,birthratereduction)
    tts.Speech()
def childdeath(childdeathno:int,childage:int,Patientsage:int,Patientsname:str)->int:
    Patients_name= #input
    childage = #input
    childdeathno= #input
    idealbreak= #input
    average = childage*Patientsage/childno
    deathratereduction= idealbreak+average
    print(Patients_name,deathratereduction)
    tts.Speech()

def percentage(deathratereduction:int, birthratereduction:int)->int:
    Patient= #input
    potencyofthepatient = (birthratereduction-deathratereduction)/100
    impotencyofthepatient(deathratereduction-birthratereduction)/100
    print(f'The potency of the {patient} is {potencyofthepatient},The impotency of the {patient} is {impotencyofthepatient}')
    tts.Speech()


